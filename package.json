{
  "name": "yago-api-server",
  "version": "0.3.0",
  "description": "Yago api server",
  "engines": {
    "node": "10.x.x"
  },
  "main": "index.js",
  "scripts": {
    "watch": "nodemon",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist",
    "dev": "cross-env NODE_ENV=development npm-run-all transpile server",
    "transpile": "babel ./src --out-dir dist",
    "clean": "rimraf dist"
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "development"
    },
    "exec": "npm run dev",
    "watch": [
      "src/*"
    ],
    "ext": "js,json"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-optional-chaining"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^2.7.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "express-async-error": "0.0.2",
    "express-jwt": "^6.0.0",
    "lodash": "~4.17.4",
    "method-override": "^3.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.8.4",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "request": "^2.81.0",
    "rimraf": "^3.0.2",
    "swagger-ui-express": "^4.1.6",
    "tape": "^4.7.0",
    "cross-env": "^5.2.0"
  },
  "license": "MIT"
}
